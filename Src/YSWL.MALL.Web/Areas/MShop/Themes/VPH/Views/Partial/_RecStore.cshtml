@model  List<YSWL.MALL.Model.Shop.Supplier.SupplierInfo>
   @if (Model != null && Model.Count > 0)
   { <div class="shop_tit clearfix">
    <h1>
        精品店铺</h1>  </div>
     <div class="shop_list shop_list_a">
                  <div> 
                @for (int i = 0; i < Model.Count; i++)
                {    
    
                        <a @if (i % 2 == 0 && i<(Model.Count-1))
                           {@:style="float: left;"
                           } href="@(ViewBag.BasePath)Store/@Model[i].SupplierId" >
                            <img  width="145" height="145" alt="@Model[i].ShopName"    src="@(ViewBag.BasePath)Partial/SuppLogo?id=@(Model[i].SupplierId)&size=T200X200" onerror="this.onerror='';this.src='/Content/themes/base/images/404/404_200X200.jpg'" class="pro"/>
                        </a>
             
                  }
                       </div>  
                  </div>  
         <div style="clear: both;"></div>
  
   }
      
            


