@{
    Layout = "/Areas/MShop/Themes/VPH/Views/Shared/_BaseLayout.cshtml";
}
@model YSWL.MALL.ViewModel.Shop.CommissionModel 
<link href="/Areas/MShop/Themes/VPH/Content/Css/UserCenter/userstyle.css" rel="stylesheet" type="text/css" />
<header id="header" class="header clearfix">
    <div class="head_left fl"><a href="javascript:;" onclick="history.go(-1);"><span class="h_back"></span></a></div>
    <span class="headtit">赚钱</span>
    <div class="head_right fr"><a href="@(ViewBag.BasePath)"><span class="h_home"></span></a></div>
</header>

<div class="comm_home">
        <div  class="s_c"><span>分销产品</span><br/><span class="red">@Model.ProductCount</span> </div><div class="line"></div>
        <div class="s_c"><span >我的盟友</span><br /><span class="red">@Model.AllyCount</span></div><div class="line"></div>
        <div class="s_c"><span>分销订单</span><br /><span class="red">@Model.OrderCount</span></div><div class="line"></div>
        <div class="s_c" ><span>我的佣金</span><br /><span class="red">@Model.AllFee.ToString("F")</span></div>
</div> 
 

<div class="s_links in_user_center">
   <a href="@(ViewBag.BasePath)u/MyQrCode"><em class="s_back"></em>我要推广</a>
   <a href="@(ViewBag.BasePath)u/MyAlly"><em class="s_bouus"></em>我的盟友</a>
</div>

  <div class="s_links in_user_center">
    <a href="@(ViewBag.BasePath)u/CommissionPro"><em class="s_wallet"></em>推广商品</a>
    <a href="@(ViewBag.BasePath)u/MyPromo"><em class="s_count"></em>我的佣金</a> 
    <a href="@(ViewBag.BasePath)Commission/CommPro"><em class="s_count"></em>佣金统计</a>
</div>

   <div class="s_links in_user_center">
    <a href="@(ViewBag.BasePath)Commission/CommPro?rl=2"><em class="s_count"></em>盟友贡献佣金</a>
    <a href="@(ViewBag.BasePath)Commission/AllyRanking"><em class="s_bouus"></em>盟友贡献排行</a>
</div>

