 @{
     Layout = "/Areas/MShop/Themes/VPH/Views/Shared/_BaseLayout.cshtml";
 }
   @model List<YSWL.MALL.Model.CMS.Content>
<header id="header" class="header clearfix">
    <div class="head_left fl"><a href="javascript:;" onclick="history.go(-1)"><span class="h_back"></span></a></div>
    <span class="headtit">新闻中心</span>
@*    <div class="head_right fr"><a href="@(ViewBag.BasePath)u"><span class="h_home"></span></a></div>*@
</header>

    <div class="connerdiv">

@if (Model != null && Model.Count > 0)
 {
    <dl class="connerbox">
        @foreach (var item in Model)
        {
            <dd style="min-height: 44px;">
                <span style="float:none;"><a href="@(ViewBag.BasePath)Article/Details/@item.ContentID">@item.Title</a></span>
                <span style="float:none;">@item.CreatedDate.ToString("yyyy-MM-dd hh:mm")</span>
            </dd>
        }
    </dl>
 }
else
{    <dl class="connerbox">
      <dd>
                <span>暂无新闻</span>
                <input name="txtuserName" id="txtPwd" type="text" value=""  class="text_input" readonly="readonly"/>
            </dd></dl>
}
 </div>
 