@model  YSWL.MALL.Model.Shop.Supplier.SupplierInfo
  <!--搜索店铺-->
    @if (Model != null)
    {

                <div class="products_list" style="border-bottom: 1px #E5E5E5 solid;">
            <ul style="height: 90px;padding: 10px 0 0 17px;">
            <li>
           <a  href="@(ViewBag.BasePath)Store/@Model.SupplierId"  class="listimg"> <img src="@(ViewBag.BasePath)Partial/SuppLogo?id=@(Model.SupplierId)&size=T200X200" onerror="this.onerror='';this.src='/Content/themes/base/images/404/404_200X200.jpg'" width="80px" height="80px"/> </a>
               
                <div style="display: inline-block;margin-top: 10px;vertical-align: top;margin-left: 18px;"> 
