        <div class="s_c" ><span>我的佣金</span><br /><span class="red">@Model.AllFee.ToString("F")</span></div>
</div> 
 
<div class="home_menu home_menu_noicon"  style="overflow: visible;">
    <ul>
        <li class="hb1"><a href="@(ViewBag.BasePath)u/MyQrCode">我要推广</a></li>
        <li class="hb2"><a href="@(ViewBag.BasePath)u/MyAlly">我的盟友</a></li>
    </ul>

    <ul>
        <li class="hb1 hb2"><a href="@(ViewBag.BasePath)u/CommissionPro">推广商品</a></li>
        <li class="hb1 hb2"><a href="@(ViewBag.BasePath)u/MyPromo">我的佣金</a></li>
        <li class="hb2"><a href="@(ViewBag.BasePath)Commission/CommPro">佣金统计</a></li>
    </ul>

    <ul>
        <li class="hb1 hb2"><a href="@(ViewBag.BasePath)Commission/CommPro?rl=2">盟友贡献佣金</a></li>
        <li class="hb2"><a href="@(ViewBag.BasePath)Commission/AllyRanking">盟友贡献排行</a></li>
    </ul>

    </div>


