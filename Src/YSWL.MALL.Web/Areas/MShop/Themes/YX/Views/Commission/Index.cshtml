@{
    Layout = "/Areas/MShop/Themes/YX/Views/Shared/_SiteLayout.cshtml";
}
@model YSWL.MALL.ViewModel.Shop.CommissionModel 
<link href="/Areas/MShop/Themes/YX/Content/Css/style.css" rel="stylesheet" type="text/css" />
<link href="/Areas/MShop/Themes/YX/Content/Css/userstyle.css" rel="stylesheet" type="text/css" />
<div class="top">
    <span>赚钱</span>
    @Html.Partial("/Areas/MShop/Themes/M1/Views/Partial/_HeadRight.cshtml")
</div>
<div class="comm_home">
        <div  class="s_c"><span>分销产品</span><br/><span class="red">@Model.ProductCount</span> </div><div class="line"></div>
        <div class="s_c"><span >我的盟友</span><br /><span class="red">@Model.AllyCount</span></div><div class="line"></div>
