@{
        Layout = "/Areas/MShop/Themes/MC01/Views/Shared/_BaseLayout.cshtml";
    }
    @model YSWL.MALL.ViewModel.Shop.CommissionModel
    @section head{
        <link href="/Areas/MShop/Themes/MC01/Content/Css/main.css" rel="stylesheet" />
        <link href="/Areas/MShop/Themes/MC01/Content/Css/user.css" rel="stylesheet" />
    }
<link href="/Scripts/vue/vux/dist/components/calendar/style.css" rel="stylesheet" />
<script src="/Scripts/vue/vux/dist/components/calendar/index.js"></script>
    <div class="main_wrapper mb55">
        <!--START HEADER-->
        <header>
            <div class="top_navbar fixed_header">
                <a href="@(ViewBag.BasePath)u">
                    <div class="si icon_goback">
                    </div>
                </a>
                <p class="header_title">赚钱</p>
            </div>
        </header>
        <!-- END HEADER -->
        <div class="top"></div>
        <div class="separate-box mt">
            <ul>
                <li>
                    <div class="name">分销产品</div>
                    <div class="number red">@Model.ProductCount</div>
                </li>
                <li>
                    <div class="name">我的盟友</div>
                    <div class="number red">@Model.AllyCount</div>
                </li>
                <li>
                    <div class="name">分销订单</div>
                    <div class="number red">@Model.OrderCount</div>
                </li>
                <li>
                    <div class="name">我的佣金</div>
                    <div class="number red">@Model.AllFee.ToString("F")</div>
                </li>
            </ul>
        </div>
        <div class="order_item mt pd">
            <a href="@(ViewBag.BasePath)u/MyQrCode" class="b_item navigate_r">
                <div class="sitem_l">
                    我要推广
                </div>
            </a>
            <a href="@(ViewBag.BasePath)u/MyAlly" class="b_item navigate_r">
                <div class="sitem_l">
                    我的盟友
                </div>
            </a>
            <a href="@(ViewBag.BasePath)u/CommissionPro" class="b_item navigate_r">
                <div class="sitem_l">
                    推广商品
                </div>
            </a>
            <a href="@(ViewBag.BasePath)u/MyPromo" class="b_item navigate_r">
                <div class="sitem_l">
                    我的佣金
                </div>
            </a>
            <a href="@(ViewBag.BasePath)Commission/CommPro" class="b_item navigate_r">
                <div class="sitem_l">
                    佣金统计
                </div>
            </a>
            <a href="@(ViewBag.BasePath)Commission/CommPro?rl=2" class="b_item navigate_r">
                <div class="sitem_l">
                    盟友贡献佣金
                </div>
            </a>
            <a href="@(ViewBag.BasePath)Commission/AllyRanking" class="b_item navigate_r">
                <div class="sitem_l">
                    盟友贡献排行
                </div>
            </a>
        </div>
    </div>
    <script type="text/javascript">
        $('#body').addClass('m_home');
    </script>