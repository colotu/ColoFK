@model List<YSWL.MALL.Model.Shop.Shipping.ShippingAddress>
@{
    Layout = "~/Areas/MBShop/Themes/Y1/Views/Shared/_BaseLayout.cshtml";
    string sku = Session["SubmitOrder_SKU"] as string;
    int count = YSWL.Common.Globals.SafeInt(Session["SubmitOrder_COUNT"], 1);
    int c = YSWL.Common.Globals.SafeInt(Session["SubmitOrder_CountDown"], 0);
    int g = YSWL.Common.Globals.SafeInt(Session["SubmitOrder_GroupBuy"], 0);
    var shopid = Request.QueryString["shopid"];
}
<header class="m_header">
    <div class="">
        <div class="m_header_bar">
            <div class="m_icon_goback icon" onclick="history.go(-1);">
                <span></span>
            </div>
            <p class="m_header_title">地址列表</p>
        </div>
    </div>
</header>
@if (Model != null && Model.Count > 0)
{
    foreach (var item in Model)
    {
        <div class="addresitem" style="overflow: hidden;position: relative;padding: .75em 10px;border-bottom: 1px solid #e1e1e1;">
            <div style="float:left;text-align:center;vertical-align:middle;display:none;/* height: .875em; */margin: 2em 0 0 0;width: 30px;" class="showselect" id="@("shop"+item.ShippingId)">
                <div addressId="@item.ShippingId">
                    <p>
                        <a href="javascript:void(0)">
                            <img src="@(ViewBag.BasePath)Areas/MBShop/Themes/Y1/Content/images/addresslist/selectright.png" style="width:20px;">
                        </a>
                    </p>
                </div>
            </div>
            <div class="item_addr" style="float:left;width: 76.125%;">
                <div class="address_info" addressId="@item.ShippingId">
                    <p>收货人:@item.ShipName</p>
                    <p>@item.Address</p>
                    <p>@item.CelPhone</p>
                </div>
            </div>
            <div class="eidt" style="float:right;text-align:center;vertical-align:middle;position: absolute;right: 3%;top: 50%;margin-top: -17px;border-left: 1px solid #c3c3c3;width: 10%;/* height: 25%; */">
                <div addressId="@item.ShippingId">
                    <p>
                        <a href="@(ViewBag.BasePath)Order/AddressInfo/@item.ShippingId">
                            <img src="@(ViewBag.BasePath)Areas/MBShop/Themes/Y1/Content/images/addresslist/addressedit.png" style="width: 1.2em;height: 1.2em;">
                        </a>
                    </p>
                </div>
            </div>
        </div>
    }
}
else
{
    <a href="@(ViewBag.BasePath)Order/AddressInfo">
        <input id="txtAddressCount" type="hidden" value="0" />
        <div class="item_addr">
            <div class="menu_item pl10">
                <span>收货信息</span>
                <div class="into icon">
                    <span></span>
                </div>
            </div>
            <div class="address_info" addressId="-1">
                <p>未设置收货信息</p>
            </div>
        </div>
    </a>
}
<script>
    $(function () {
        $('#shop'+ '@shopid').show();
        $('.address_info').click(function () {
            $('.showselect').hide();
            $(this).parent('div').siblings('.showselect').show();
            var shopid = $(this).attr('addressId');
            window.location.href = "@(ViewBag.BasePath)Order/SubmitOrder?sku=@sku"+"&count=@count"+"&c=@c"+"&g=@g"+"&shopid="+shopid;
        });
    });
</script>

