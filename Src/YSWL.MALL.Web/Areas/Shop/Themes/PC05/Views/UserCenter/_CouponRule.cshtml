@model List<YSWL.MALL.Model.Shop.Coupon.CouponRule>

@if (Model != null && Model.Count > 0)
{
    <div class="form search-01" style="margin-bottom: 20px">
       <div style="float:left;width: 682px;">
        <ul class="list-h">
            @foreach (var item in Model)
            {
                    <li style="height: 25px;
line-height: 25px;">
                <input  type="radio" class="radExc radio" name="RadioGroup1" value="@item.CouponPrice" ruleId="@item.RuleId" point="@item.NeedPoint"  >@(item.Name)(需@(item.NeedPoint)积分)</li>
            }
        </ul>
        </div>
        
         <div id="liDo" style="float:right;margin-top: 5px;">
                <input  style="margin-right: 10px;" type="button" value="兑&#12288;&#12288;换"  id="btnExchange" class="reg-bt"></div>
</div>
}           

