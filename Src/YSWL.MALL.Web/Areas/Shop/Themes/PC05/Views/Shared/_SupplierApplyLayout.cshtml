@{
    Layout = "~/Areas/Shop/Themes/PC05/Views/Shared/_SiteLayout.cshtml";
@*商家申请环节模版*@
    }
@section head{
<link href="/Areas/Shop/Themes/PC05/Content/css/shopc2.css" rel="stylesheet" type="text/css" />
<link href="/Areas/Shop/Themes/PC05/Content/Css/goods.css" rel="stylesheet" type="text/css" />
    @RenderSection("head", false)
}
 
<div class="top-bg  top-bg2" >
  @Html.Action("Header", "Partial", new { area = "Shop" }) 
  <div class="top-logo top-logo1 clearfix">
       @Html.Action("Header", "Partial", new { area = "Shop", ViewName = "_HeaderShopping" })
       @RenderSection("step", false)
  </div>
 
</div>
 
<div class="sub-goodsbody sub-goodsbody1 clearfix">
@RenderBody()
</div>

@Html.Action("Footer", "Partial", new { area = "Shop" })
@section foot{
    @RenderSection("foot", false)
}

<script  type="text/javascript">
    $('#div_header_top').addClass('top-logo1');
    $('.foot').addClass('foot1');
</script>