@model List<YSWL.MALL.Model.CMS.Content>
       @if (Model != null && Model.Count > 0)
       {
           foreach (var item in Model)
           {
                <dd><a href="@(ViewBag.BasePath)Article/Details/@item.ContentID">@(YSWL.Common.StringPlus.SubString(item.Title, 8, ".."))</a></dd>
           }

       }
       else
       {
           <dd> <span style="margin-left: 25px;color: gray;">暂无内容</span></dd>
       }
          