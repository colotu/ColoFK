  @model List<YSWL.MALL.Model.Shop.Products.CategoryInfo>

		                	<div class="productalls-left">
                             @if (Model != null && Model.Count > 0)
                            {   
                                 foreach (var item in Model)
                                 {
                                    <dl>
                            <dt>
                            <a href="@(ViewBag.BasePath)product/@item.CategoryId"  target="_blank">@item.Name</a>
                            </dt>
                            <dd>
                               @Html.Action("CategoryList", "Partial", new { Cid = item.CategoryId, Top = 0, viewName = "_CateThree" })
                             </dd>
                             </dl>
                                 }
                            }
                            </div>
		                  