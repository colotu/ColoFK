@model List<YSWL.MALL.Model.Settings.Advertisement>
<div class="rqrm_c e-cms" id="rqrm_c" onmouseover="controlScroll(2)" onmouseout="controlScroll(1)" e_cms="slide" e_cdata="hostLoorList" style="display: none;">
    @if (Model != null)
                 {
                     for (int i = 0; i < Model.Count;i++)
                     {
                         if (i == 0) { 
        <a href="@Model[i].NavigateUrl" target="_blank">
            <img src="@Model[i].FileUrl"
                width="205" height="299">
        </a>
                         }
                         else if (i == 1) { 
        <a href="@Model[i].NavigateUrl" target="_blank">
            <img src="@Model[i].FileUrl"
                width="590" height="299" class="cimg"></a>
                         }
                         else if (i == 2) {
        <a href="@Model[i].NavigateUrl" target="_blank">
            <img src="@Model[i].FileUrl"
                width="200" height="299" class="cimg"></a>
				        
                         }
                         else if (i == 3)
                         {    <a href="@Model[i].NavigateUrl" target="_blank">
                             <img src="@Model[i].FileUrl"
                                 width="325" height="250" class="timg"></a>
                         }
                         else if (i == 4)
                         {
        <a href="@Model[i].NavigateUrl" target="_blank">
            <img src="@Model[i].FileUrl"
                width="465" height="250" class="cimg timg"></a>
							
                         }
                         else if (i == 5)
                         { <a href="@Model[i].NavigateUrl" target="_blank">
                             <img src="@Model[i].FileUrl"
                                 width="205" height="250" class="cimg timg"></a>

                         }
                     }
                 }
</div>
