@model List<YSWL.MALL.Model.Shop.Products.CategoryInfo>
                 @if (Model != null && Model.Count > 0)
                 {
                     for (int i = 0; i < Model.Count; i++)
                     {
<li><a href="@(ViewBag.BasePath)product/@Model[i].CategoryId" target="_blank">@Model[i].Name</a></li>
                         if ((i + 1) < Model.Count)
                         {
                         <li class="i">|</li>
                        }								 					
                     }
                 }


           


