    <div id="wk-2d-bar-code" style="display: block; position: fixed; top: 280px; right: 0px;z-index: 9999;">
        <div class="float">
            <span class="close-btn" title="关闭"></span>
            <div style="width: 138px">
                <span style="float: left; padding-bottom: 12px; padding-left: 20px; margin-top: -10px">
                    扫描访问手机版</span>
                <img class="wk-2d-bar-code-qrImg" width="120px" onerror="wk2dbarcodeqrImgErr(this)" />
            </div>
            <div style="width: 138px">
                <span style="float: left; padding-bottom: 12px; padding-left: 20px; padding-top: 12px">
                    扫描下载安卓客户端</span>
                <img class="wk-2d-bar-code-qrImg" width="120px" onerror="wk2dbarcodeqrImgErr(this)" />
            </div>
        </div>
    </div>
    <script type="text/javascript">
        $('.wk-2d-bar-code-qrImg:eq(0)').attr('src', '/@(YSWL.Components.MvcApplication.UploadFolder)/QR/website.png?r=' + Math.random());
        $('.wk-2d-bar-code-qrImg:eq(1)').attr('src', '/@(YSWL.Components.MvcApplication.UploadFolder)/QR/android.png?r=' + Math.random());

        function wk2dbarcodeqrImgErr(sender) {
            $(sender).attr('state', 'null').parent().hide();
            if ($('.wk-2d-bar-code-qrImg[state]').length == 2) {
                $('#wk-2d-bar-code').hide();
            }
        }
    </script>
</div>
