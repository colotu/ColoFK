                location.replace('/m');
            }
        } catch(e) { }
    </script>
}
<script type="text/javascript">
    $(function () {
        $(".divOpenAct").click(function () {
            var tip = $(this).children().text(); 
            if (tip == "展开") {
                $(this).children().text("收起");
            } else {
                $(this).children().text("展开");
            }
            $(".divContent").toggle();
        });
    })
</script>
