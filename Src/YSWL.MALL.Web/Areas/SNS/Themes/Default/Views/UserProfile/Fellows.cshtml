@{
    Layout = "~/Areas/SNS/Themes/Default/Views/Shared/_UserProfileLayOut.cshtml";
}
@using Webdiyer.WebControls.Mvc
@model PagedList<YSWL.MALL.Model.SNS.UserShip>
@section head{
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")"></script>
 <script src="/Areas/SNS/Themes/Default/Content/Scripts/FellowUser.js" type="text/javascript"></script>
 }
