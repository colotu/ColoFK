           @model List<YSWL.MALL.Model.SNS.UserBlog>
           
           @if (Model != null && Model.Count > 0)
           {
               <dl class="single_online_ht">
                   @foreach (var item in Model)
                   {
                      <dl class="hts">
                         <dt><a href="@(ViewBag.BasePath)User/Posts/@item.UserID"><img alt="@item.UserName" style=" height:50px; width:50px" src="/@(YSWL.Components.MvcApplication.UploadFolder)/User/Gravatar/@(@item.UserID).jpg"/></a></dt>
                         <dd class="hts_a"><a href="@(ViewBag.BasePath)User/Posts/@item.UserID">@item.UserName</a></dd>
                      </dl>
                   }
                     
                   </dl>
           }

