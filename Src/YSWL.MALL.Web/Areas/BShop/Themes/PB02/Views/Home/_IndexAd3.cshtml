@model List<YSWL.MALL.Model.Settings.Advertisement>
@if (Model != null && Model.Count > 0)
{
    for (int i=0;i< Model.Count;i++)
    {
        <a href="@Model[i].NavigateUrl"  target="_blank" class="sidebar-ad @(i>0?"module":"")">
            <img src="@Model[i].FileUrl" alt="@Model[i].AdvertisementName">
        </a>
    }
}

