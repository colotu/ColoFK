@{
    Layout = "/Areas/MB/Themes/MB02/Views/Shared/_BaseLayout.cshtml";
    ViewBag.Title = "我要推广";
}
@section head{
    <link href="/Areas/MB/Themes/MB02/Content/Css/main.css?v=1.2" rel="stylesheet" />
}
<div class="main_wrapper mb55">
    <!--START HEADER-->
    <header>
        <div class="top_navbar fixed_header">
            @Html.Partial("/Areas/MB/Themes/MB02/Views/Partial/_Back.cshtml")         
            <p class="header_title">我要推广</p>
        </div>
    </header>
    <div class="top"></div>
    <style type="text/css">
        .qrImg {
            width: 100%;
            border: 0;
            left: 0;
        }
    </style>
    <div>
        @{
            if (ViewBag.IsExist)
            {
                <img class="qrImg" src="/@(YSWL.Components.MvcApplication.UploadFolder)/QRcode/QR_@(ViewBag.UserId).jpg" alt="推广二维码" />
            }
            else
            {
                <div>
                    <img class="qrImg" src="/Images/QR_notExist.png" alt="推广二维码不存在" />
                </div>
            }
        }
    </div>
</div>
