@model  List<YSWL.MALL.ViewModel.Order.SuppSaleInfo>
@if (Model != null)
{
    for (int i = 0; i < Model.Count; i++)
    {
        <div class="rank-line clearfix">
                <span class="rank-num">@(i+1)</span>
                <div class="company-name">
                    @Model[i].Name
                </div>
                <div class="number">@Model[i].Count</div>
            </div>
    }
}
else
{
    <div class="home_right_list_empty">暂无数据</div>
}