@model List<YSWL.MALL.Model.Shop.Supplier.SuppAreas>
@if (Model != null)
{
    for (int i = 0; i < Model.Count; i++)
    {
<div class="building-box @(i==0?"open":"") ">
    <div class="building-item">
        <div class="building-name">@Model[i].Name</div>
        <div class="extra-info">玩具，珠宝，首饰，花类，装饰工艺，相框</div>
    </div>
    <div class="floor-info">
        @Html.Action("SuppAreasList", "Partial", new { AreaId = Model[i].AreaId, Top = 0, viewName = "/Areas/MB/Themes/MB02/Views/Home/_SuppAreasSecondList.cshtml" })
    </div>
</div>
    }
}
