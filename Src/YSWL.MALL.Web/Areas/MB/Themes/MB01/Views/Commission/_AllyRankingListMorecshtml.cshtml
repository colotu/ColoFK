@using Webdiyer.WebControls.Mvc
@model PagedList<YSWL.MALL.ViewModel.Shop.CommissionStat>
@if (Model != null && Model.Count > 0)
{
    foreach (var item in Model)
    {
        <div class="list-item">
            <div class="text-wrap">
                <div class="list-line">
                    盟友：
                    <span class="intro">@item.NickName</span>
                </div>
                <div class="list-line">
                    佣金：
                    <span class="intro price">￥@item.TotalFee.ToString("F")</span>
                </div>
            </div>
        </div>
    }
}